    "@rin/api": "workspace:*",
    "@tailwindcss/typography": "^0.5.13",
    "@types/lodash": "^4.17.7",
    "@types/react-modal": "^3.16.3",
    "@types/react-syntax-highlighter": "^15.5.13",
    "dompurify": "^3.2.7",
    "i18next": "^25.8.10",
    "i18next-browser-languagedetector": "^8.0.0",
    "i18next-http-backend": "^3.0.2",
    "lodash": "^4.17.21",
    "mermaid": "^11.12.3",
    "primereact": "^10.6.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-i18next": "^16.5.4",
    "react-markdown": "^10.1.0",
    "react-modal": "3.16.3",
    "react-select": "5.10.2",
    "react-syntax-highlighter": "^16.1.0",
    "reactjs-popup": "^2.0.6",
    "rehype-katex": "^7.0.0",
    "rehype-mathjax": "^7.1.0",
    "rehype-parse": "^9.0.0",
    "rehype-raw": "^7.0.0",
    "rehype-react": "^8.0.0",
    "rehype-sanitize": "^6.0.0",
    "remark-gfm": "^4.0.0",
    "remark-github-blockquote-alert": "^2.0.1",
    "remark-math": "^6.0.0",
    "remark-rehype": "^11.1.0",
    "remixicon": "^4.2.0",
    "typescript-cookie": "^1.0.6",
    "wouter": "3.9.0",
    "yet-another-react-lightbox": "^3.21.1"
