{
  "name": "client",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "bunx --bun vite --open /",
    "build": "tsc && vite build",
    "build:analyze": "tsc && vite build --mode analyze",
    "build:analyze:open": "VISUALIZER_OPEN=true bun run build:analyze",
    "check": "tsc",
    "lint": "eslint . --max-warnings 0",
    "preview": "vite preview",
    "i18n": "i18next",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:coverage:ci": "vitest run --coverage --coverage.reporter=text --coverage.reporter=lcov --coverage.reportsDirectory=coverage --reporter=default --reporter=junit --outputFile.junit=coverage/junit.xml"
  },
  "dependencies": {
    "@astroimg/timeago": "^1.1.6",
    "@monaco-editor/react": "4.7.0",
    "@prosemirror-adapter/react": "^0.2.6",
